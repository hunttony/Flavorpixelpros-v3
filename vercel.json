{
    "version": 2,
    "framework": null,

    "builds": [
      {
        "src": "/api/(*.js)",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/", "dest": "/api/index.js" },
    
        {
          "src": "/api",
          "dest": "/api/index.js",
          "methods": ["GET", "OPTIONS"]
        },
        {
          "src": "/api/contact",
          "dest": "/api/contact.js",
          "methods": ["POST", "OPTIONS"]
        },
        {
          "src": "/api/health",
          "dest": "/api/health.js",
          "methods": ["GET", "OPTIONS"]
        },
        {
          "src": "/contact",
          "dest": "/api/contact.js",
          "methods": ["POST", "OPTIONS"],
          "status": 308,
          "headers": {
            "Location": "/api/contact"
          }
        }
      
    ],

    

    "env": {
      "NODE_ENV": "production"
    }
  }