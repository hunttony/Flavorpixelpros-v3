{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "/api/health.js",
      "use": "@vercel/node"
    },
    {
      "src": "/api/contact.js",
      "use": "@vercel/node"
    },

    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "/api/index.js",
      "methods": ["GET", "OPTIONS"]
    },
    {
      "src": "/api/contact",
      "dest": "/api/contact.js",
      "methods": ["POST"]
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js",
      "methods": ["GET", "OPTIONS"]
    }
    
  ],
  "env": {
    "NODE_ENV": "production"
  }
}